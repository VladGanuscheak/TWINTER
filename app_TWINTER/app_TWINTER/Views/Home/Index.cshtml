@{
    ViewBag.Title = "TWINTER";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>TRANDING RIGHT NOW</title>
</head>
<body>
    <h3>Welcome!!!</h3>
    <div align="right">
        <table>
            <tr>
                <th>TRANDING RIGHT NOW!!!</th>
            </tr>
            @foreach (var tr in ViewBag.trandings)
            {
                <tr>
                    <td>@tr.HashTag</td>
                </tr>
            }
        </table>
    </div>
    @{ViewBag.Id = 1; }
    <a href="~/Views/Account/Index.cshtml">My Account</a>

    @Html.ActionLink("Account", "Index");

</body>
</html>