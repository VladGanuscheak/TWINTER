
@{
    ViewBag.Title = "UserBioList";
    Layout = "~/Views/Shared/MainAdminLayout.cshtml";
}

<h2>User's BIO List</h2>
<table align="center">
    <tr>
        <td>User_Id</td>
        <td>Bio_Id</td>
    </tr>
    @foreach (var item in ViewBag.UserBIOList)
    {
        <tr>
            <td>@item.User_Id</td>
            <td>@item.BIO_Id</td>
        </tr>
    }
</table>
