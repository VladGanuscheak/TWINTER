
@{
    ViewBag.Title = "MediaList";
    Layout = "~/Views/Shared/MainAdminLayout.cshtml";
}

<h2>Media List</h2>
<table align="center">
    <tr>
        <td>media_ID</td>
        <td>Type</td>
        <td>URL</td>
    </tr>
    @foreach (var item in ViewBag.Media)
    {
        <tr>
            <td>@item.media_Id</td>
            <td>@item.media_type</td>
            <td>@item.URL</td>
        </tr>
    }
</table>
